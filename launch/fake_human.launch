<launch>
    <arg name="human_task_num" default="1" />
    <arg name="debug" default="false" />
    <param  name="human_task_num" value="$(arg human_task_num)"/>
    <arg unless="$(arg debug)" name="launch_prefix" value="" />
    <arg     if="$(arg debug)" name="launch_prefix"
           value="gdb --ex run --args" /> 
    <arg name="transform" default="0,0,0,1,0,0,0" />
    <param  name="plan_group" value="edo"/>
    <param  name="ctrl_ns" value="edo"/>

    <node name="sim_human" pkg="fake_human" type="sim_human" respawn="false" output="screen" launch-prefix="$(arg launch_prefix)">
        <param name="workcell_transform" value="$(arg transform)"/>
    </node> 
    <!-- launch-prefix="gdb -ex run  -args"> -->
    <!-- "gdb -.-ex run -.-args"  -->
</launch>
